                special(SPC_MOD)
                interest(1)
        }
        field(BDST,DBF_DOUBLE) {
                prompt("Backlash Distance (EGU)")
                promptgroup(GUI_COMMON)
                special(SPC_MOD)
                interest(1)
                asl(ASL0)
        }
