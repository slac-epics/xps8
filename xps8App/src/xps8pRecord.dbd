                special(SPC_NOMOD)
        }
        field(HVEL,DBF_DOUBLE) {
                prompt("Homing V (EGU/s)")
                promptgroup(GUI_COMMON)
                special(SPC_MOD)
                interest(1)
		initial("0.4")
