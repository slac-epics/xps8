recordtype(xps8p) {
	include "dbCommon.dbd"
	field(HOST,DBF_STRING) {
		prompt("Name of Host Machine")
		special(SPC_NOMOD)
		interest(1)
		size(61)
	}
	field(IOCN,DBF_STRING) {
		prompt("Linux Name of IOC")
		special(SPC_NOMOD)
		interest(1)
		size(61)
	}
        field(CARD,DBF_SHORT) {
                prompt("Card Number")
                special(SPC_NOMOD)
                interest(1)
        }
	field(PTYP,DBF_STRING) {
		prompt("Previous Type of the Stage")
		special(SPC_NOMOD)
		interest(1)
		size(61)
	}
	field(TYPE,DBF_STRING) {
		prompt("Type of the Stage")
		special(SPC_NOMOD)
		interest(1)
		size(61)
	}
	field(DSTA,DBF_LONG) {
		prompt("Driver Status")
		special(SPC_NOMOD)
		interest(1)
		initial("-1")
	}
	field(DSTR,DBF_NOACCESS) {
		prompt("Driver Status String")
		special(SPC_DBADDR)
		extra("char *dstr")
		interest(1)
	}
	field(HSTA,DBF_LONG) {
		prompt("Hardware Status")
		special(SPC_NOMOD)
		interest(1)
		initial("-1")
	}
	field(HSTR,DBF_NOACCESS) {
		prompt("Hardware Status String")
		special(SPC_DBADDR)
		extra("char *hstr")
		interest(1)
	}
	field(SNUM,DBF_LONG) {
		prompt("State Number")
		special(SPC_NOMOD)
		interest(1)
		initial("-1")
	}
	field(SSTR,DBF_NOACCESS) {
		prompt("State String")
		special(SPC_DBADDR)
		extra("char *sstr")
		interest(1)
	}
	field(ERR,DBF_LONG) {
		prompt("Error Number")
		special(SPC_NOMOD)
		interest(1)
		initial("999")
	}
	field(ESTR,DBF_NOACCESS) {
		prompt("Error String")
		special(SPC_DBADDR)
		extra("char *estr")
		interest(1)
	}
	field(KILL,DBF_SHORT) {
		prompt("Reset")
		special(SPC_MOD)
		interest(1)
	}
	field(INIT,DBF_SHORT) {
		prompt("Initialize")
		special(SPC_MOD)
		interest(1)
	}
        field(HOME,DBF_SHORT) {
                prompt("Home Search")
                special(SPC_MOD)
                interest(1)
        }
        field(REFR,DBF_SHORT) {
                prompt("Reference to DRBV")
                special(SPC_MOD)
                interest(1)
        }
        field(REF2,DBF_DOUBLE) {
                prompt("Reference to Value")
                special(SPC_MOD)
                interest(1)
        }
        field(DIR,DBF_MENU) {
                prompt("User Direction")
                promptgroup(GUI_COMMON)
                menu(xps8pDIR)
                special(SPC_MOD)
                initial("Positive")
                interest(1)
        }
        field(OFF,DBF_DOUBLE) {
                prompt("User Offset (EGU)")
                special(SPC_MOD)
                asl(ASL0)
        }
        field(SET,DBF_MENU) {
                prompt("Use/Set Switch")
                menu(xps8pSET)
                special(SPC_MOD)
                interest(1)
                asl(ASL0)
        }
