record(calcout, "$(DEVICE):STUP") {
  field(DESC, "Status Update Watcher")
  field(INPA, "$(DEVICE).STUP CPP")
  field(INPB, "$(DEVICE):UpdateClock.SCAN NPP")
  field(OUT,  "$(DEVICE):STUP_Seq.PROC")
  field(CALC, "A")
  field(OCAL, "1")
  field(OOPT, "On Change")
  field(DOPT, "Use OCAL")
}

record(seq, "$(DEVICE):STUP_Seq") {
  field(DESC, "Status Update Sequencer")
  field(SELM, "All")
  field(LNK1, "$(DEVICE):UpdateClock.SCAN")
  field(DO1,  "0")
  field(DLY1, "0")
  field(LNK2, "$(DEVICE):UpdateEvtGen.VAL PP")
  field(DO2,  "1")
  field(DLY2, "1")
  field(LNK3, "$(DEVICE):UpdateEvtGen.VAL PP")
  field(DO3,  "2")
  field(DLY3, "2")
  field(LNK4, "$(DEVICE):UpdateEvtGen.VAL PP")
  field(DO4,  "3")
  field(DLY4, "2")
  field(LNK5, "$(DEVICE):UpdateEvtGen.VAL PP")
  field(DO5,  "4")
  field(DLY5, "2")
  field(LNK6, "$(DEVICE):UpdateEvtGen.VAL PP")
  field(DO6,  "5")
  field(DLY6, "2")
  field(LNK7, "$(DEVICE):UpdateEvtGen.VAL PP")
  field(DO7,  "6")
  field(DLY7, "2")
  field(LNK8, "$(DEVICE):UpdateEvtGen.VAL PP")
  field(DO8,  "7")
  field(DLY8, "2")
  field(LNK9, "$(DEVICE):UpdateEvtGen.VAL PP")
  field(DO9,  "8")
  field(DLY9, "2")
  field(LNKA, "$(DEVICE):UpdateClock.SCAN")
  field(DOLA, "$(DEVICE):STUP.B")
  field(DLYA, "5")
}

