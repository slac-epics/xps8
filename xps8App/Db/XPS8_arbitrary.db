record(xps8, "$(DEVICE)") {
  field(CTRL, "$(CTRL)")
  field(PORT, "$(PORT)")
  field(SCAN, "Event" )
  field(EVNT, "9" )
  field(FLNK, "$(DEVICE):TS")
}

record(stringin, "$(DEVICE):TS") {
  field(DESC, "Time Stamp of Last Update")
  field(DTYP, "Soft Timestamp")
  field(TSEL, "$(DEVICE).TIME")
  field(INP,  "@%b %d %R:%S %Y")
  field(FLNK, "$(DEVICE):UTCS")
}

record(stringin, "$(DEVICE):UTCS") {
  field(DESC, "UTC String of Last Update")
  field(DTYP, "Soft Timestamp")
  field(TSEL, "$(DEVICE).TIME")
  field(INP,  "@%s")
  field(FLNK, "$(DEVICE):UTC")
}

record(scalcout, "$(DEVICE):UTC") {
  field(DESC, "UTC Seconds of Last Update")
  field(INAA, "$(DEVICE):UTCS")
  field(CALC, "INT(AA)")
}

